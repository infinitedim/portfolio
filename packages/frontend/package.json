{
  "name": "@portfolio/frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -H 127.0.0.1",
    "dev:https": "concurrently -k -s first -n \"next,ssl\" -c \"green,magenta\" \"next dev -p 3000\" \"local-ssl-proxy --key ./localhost-key.pem --cert ./localhost.pem --source 3443 --target 3000\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"./**/*.{js,jsx,md,mdx,ts,tsx}\"",
    "lint:fix": "eslint \"./**/*.{js,jsx,md,mdx,ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "browserslist": {
    "production": [
      ">0.01%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.01%",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
