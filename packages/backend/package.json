{
  "name": "@portfolio/backend",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./trpc/router": "./src/trpc/router.ts",
    "./trpc/context": "./src/trpc/context.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "start": "bun x nest start",
    "dev": "bun x nest start --watch",
    "build": "bun x nest build",
    "lint": "bun x eslint . --ext .ts",
    "lint:fix": "bun x eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "type-check": "bun x tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "hash-password": "bun run scripts/hash-password.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.53",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/throttler": "^6.4.0",
    "@portfolio/logger": "workspace:*",
    "@portfolio/trpc": "workspace:*",
    "@prisma/client": "7.1.0",
    "@prisma/config": "^7.0.1",
    "@trpc/client": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@types/pg": "^8.15.6",
    "@upstash/redis": "^1.35.7",
    "@vercel/node": "^5.5.13",
    "ai": "^5.0.108",
    "bcryptjs": "^3.0.3",
    "cache-manager": "^7.2.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ipaddr.js": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "nest-winston": "^1.10.2",
    "next": "^16.0.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.13",
    "@nestjs/testing": "^11.1.9",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^24.10.1",
    "sanitize-html": "^2.17.0",
    "ts-node": "^10.9.2"
  }
}
