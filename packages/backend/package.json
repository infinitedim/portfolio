{
  "name": "@portfolio/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "bun x nest start",
    "dev": "bun x nest start --watch",
    "build": "bun x nest build",
    "lint": "bun x eslint . --ext .ts",
    "lint:fix": "bun x eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "type-check": "bun x tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.7",
    "@nestjs/core": "^10.4.7",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/config": "^4.0.0",
    "@prisma/client": "^5.17.0",
    "@trpc/client": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@upstash/redis": "^1.31.6",
    "@vercel/node": "^5.3.12",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.1.1",
    "express": "^4.19.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nest-winston": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.17.0",
    "zod": "^3.24.1",
    "@portfolio/trpc": "workspace:*",
    "@portfolio/logger": "workspace:*"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/testing": "^10.4.7",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.8",
    "sanitize-html": "^2.17.0",
    "jsdom": "^26.1.0",
    "vitest": "^3.2.4"
  }
}
